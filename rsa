#!/usr/bin/python

import sys

# Function to check if a number is prime
def is_prime(n):
    if n <= 1:
        return False
    if n <= 3:
        return True
    if n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True

# Function to find the prime factors of n
def find_prime_factors(n):
    factors = []
    for i in range(2, n + 1):
        if n % i == 0 and is_prime(i):
            factors.append(i)
            n //= i
            while n % i == 0:
                factors.append(i)
                n //= i
            if n == 1:
                break
    return factors

# Function to format the output
def format_output(n, factors):
    return f"{n}={'*'.join(map(str, factors))}"

# Main function to read numbers from file and find prime factors
def main(file_path):
    with open(file_path, 'r') as file:
        numbers = file.readlines()

    for number in numbers:
        n = int(number.strip())
        factors = find_prime_factors(n)
        print(format_output(n, factors))

# Check if the script is being run as the main program
if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: ./rsa <file>")
        sys.exit(1)
    file_path = sys.argv[1]
    main(file_path)

